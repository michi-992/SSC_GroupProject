<html>
<head>
    <%- include('htmlHead') %>
</head>
<body>
<%- include('navComponent') %>

<h1>These are all <%= users.length %> Users</h1>
<div class="userCards">
    <% for (let i = 0; i < users.length; i++) { %>
        <div class="userCard">
            <a href="/users/<%= users[i].id %>"><%= users[i].username %> <%= users[i].nation ? ' (' + users[i].nation + ') ' : '' %></a>
        </div>
    <% } %>
</div>


</body>
</html>