<div id="userCards">
    <% for (let i = 0; i < members.length; i++) { %>
        <div class="displayCard">
            <p class="label">name</p>
            <p class="userInformation"> <%= members[i].name %> </p>
            <p class="label">age</p>
            <p class="userInformation"> <%= members[i].age %> </p>
            <p class="label">coding experience</p>
            <p class="userInformation"> <%= members[i].codingExperience %> </p>
            <p id="catchphrase"> <%= members[i].catchphrase %> </p>
        </div>
    <% } %>
</div>

